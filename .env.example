# .env.example

# Variables de la aplicación Kairos BCP
DATABASE_URL="postgresql+asyncpg://${DB_USER}:${DB_PASSWORD}@localhost:5432/${DB_NAME}" # URL de conexión a la base de datos PostgreSQL
GOOGLE_API_KEY="" # Ejemplo: para servicios de Google (si los usas)
LLM_MODEL=""      # Ejemplo: identificador del modelo LLM a utilizar
AGENT_TEMPERATURE="" # Ejemplo: parámetro de configuración para el LLM

# Variables para la base de datos (usadas por docker-compose.yml y la aplicación)
DB_NAME="kairos_db" # Nombre de la base de datos
DB_USER="kairos_user" # Usuario de la base de datos
DB_PASSWORD="strong_password_change_me" # Contraseña de la base de datos

# Variables para pgAdmin (usadas por docker-compose.yml)
PGADMIN_EMAIL="admin@example.com" # Email de acceso a pgAdmin
PGADMIN_PASSWORD="strong_admin_password_change_me" # Contraseña de acceso a pgAdmin
